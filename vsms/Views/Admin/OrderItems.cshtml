@{
    Layout = "_Layout1";
}
<section class="subscribe_section">
    <div class="container-fuild">
        <div class="box">
            <div class="row">
                <div class="col-md-6 offset-md-3">
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Id</th>
                                <th scope="col">Quantity</th>
                                <th scope="col">Order Date</th>
                                <th scope="col">Vehicle Name</th>

                            </tr>
                        </thead>
                        <tbody>


                            @foreach (var value in ViewBag.orders)
                            {
                                <tr>

                                    <td> @value.OrderItemId </td>
                                    <td>@value.Quantity</td>
                                    <td>@value.Order.DateOfPurchase</td>
                                    <td>@value.Vehicle.CompanyVehicleName</td>

                                </tr>

                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>